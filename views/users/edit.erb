<h1>OK SO YOU WANT TO EDIT THIS USER??? CHANGE IT HERE</h1>

<form action="/literally_music/users/<%= @user.id %>" method="post">
  <label for="user-name">Name:</label>
    <input type="text" name="user_name" id= "user-name" value= <%= @user.user_name %>>
  <label for="email-address">Email Address:</label>
    <input type="text" name="email_address" id="email-address" value= <%= @user.email_address %>>

  <input type="submit" value="Edit user">
    <hr>
</form>
